    "def reverse(str):\n",
    "    l=\"\"\n",
    "    for i in str:\n",
    "        l=i+l\n",
    "    return l\n",
    "mystr = \"1234abcd\"\n",
    "print(\"string:\", mystr)\n",
    "print(\"reversed String:\", reverse(mystr))"
