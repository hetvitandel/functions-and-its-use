    "def count(s):\n",
    "    upper=0\n",
    "    lower=0\n",
    "    for i in s:\n",
    "        if i >= 'A' and i <= 'Z':\n",
    "            upper += 1\n",
    "        elif i >= 'a' and i <= 'z':   \n",
    "            lower += 1\n",
    "    print(\"upper case:\" + str(upper))\n",
    "    print(\"lower case:\" + str(lower))\n",
    "count(\"My Name Is HETVI\")"
   ]
